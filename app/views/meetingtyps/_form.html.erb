<%= semantic_form_for [@meetingtyp] do |f| %>
  <%= f.inputs do %>
      <%= f.input :name,   :placeholder=> "Name" %>
      <%= f.input :rubrik %>
      <%= f.input :agendaintern %>
      <%= f.input :protocolintern %>
      <%= image_tag @meetingtyp.picture.thumb.url %>
      <%= f.input :picture %>

  <% end %>
  <%= f.actions do %>
  <%= f.action :submit, :as => :input %>
  <% end %>

<% end %>
