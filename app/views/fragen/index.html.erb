<h1>Listing fragen</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @fragen.each do |frage| %>
  <tr>
    <td><%= frage.title %></td>
    <td><%= frage.text %></td>
    <td><%= link_to 'Show', frage %></td>
    <td><%= link_to 'Edit', edit_frage_path(frage) %></td>
    <td><%= link_to 'Destroy', frage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

